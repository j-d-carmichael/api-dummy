{
  "name": "api-dummy",
  "description": "example generate-it",
  "version": "1.0.0",
  "private": true,
  "author": "John Carmichael",
  "scripts": {
    "build": "ttsc -p .",
    "cli-script": "VERBOSE=true && node ./build/server.js -r ",
    "dev:cli-script": "npm run build && VERBOSE=true && node ./build/server.js -r ",
    "dev:setup": "git config core.hooksPath githooks && cp -n .env.example .env",
    "dev:build-watch": "ttsc --watch -p .",
    "dev:start-watch": "nodemon ./build/server.js",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "start": "node ./build/server.js",
    "npm-update-git-deps": "npm-update-git-deps",
    "pm2:start:dev": "pm2 start pm2.dev.config.js",
    "pm2:start:prod": "pm2 start pm2.prod.config.js",
    "test": "npm run build && npm run lint && npm run test:unit",
    "test:unit": "ENVIRONMENT=test && jest --silent --runInBand",
    "test:unit:single": "jest --clearCache && ENVIRONMENT=test && jest --collect-coverage false --verbose --forceExit --detectOpenHandles",
    "generate:server": "generate-it ../api-dummy_d/build/api-dummy_d_1.0.1.yml -m -u -y -t https://github.com/acrontum/openapi-nodegen-typescript-server.git"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "celebrate": "^15.0.0",
    "command-line-args": "^5.2.0",
    "cors": "^2.8.5",
    "custom-error": "^0.2.1",
    "dotenv": "^14.2.0",
    "express": "^4.17.3",
    "express-auth-middle": "^1.1.2",
    "express-form-data": "^2.0.17",
    "express-promise-router": "^4.1.1",
    "generate-it": "github:acr-lfr/generate-it#feature/injections",
    "generate-it-logger": "^1.2.7",
    "generate-it-mockers": "^1.1.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "node-worker-threads-pool": "^1.5.1",
    "npm-update-git-deps": "^1.3.0",
    "object-reduce-by-map": "^2.1.0",
    "openapi-nodegen-config-helper": "^1.3.1",
    "recursive-readdir-sync": "^1.0.6",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^2.1.3",
    "swagger-ui-express": "^4.3.0",
    "tslib": "^2.3.1",
    "typeorm": "^0.2.45",
    "uuid": "^8.3.2",
    "winston": "^3.4.0",
    "worker-farm": "^1.7.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.1",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.13",
    "@types/express-form-data": "^2.0.2",
    "@types/fs-extra": "^9.0.12",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/lodash": "^4.14.171",
    "@types/morgan": "^1.9.3",
    "@types/node": "^14.17.5",
    "@types/request-ip": "^0.0.37",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^8.3.1",
    "@types/validator": "^13.6.3",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecate": "^0.7.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.4",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.1",
    "supertest": "^6.1.3",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.1.0",
    "ttypescript": "^1.5.12",
    "typescript": "~4.3.5"
  },
  "mockingServer": true,
  "main": "index.js",
  "repository": {
    "type": "git",
